<%= link_to categories_path do %>
<%= image_tag("logo.png", class: "menu-icon") %>
<%end%>
<h1> New Timer Form </h1>


<h1> Choose the times you want reality checks sent!<h1>
<%= form_for [@category, @timer] do |f| %>

    <%= f.label :from %><br>
    <%= f.select :rate, [ 1, 5, 12 ], :prompt => 'Select hourly rate' %>

    <%= f.submit %>
<% end %>
